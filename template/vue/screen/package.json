  "pnpm": {
    "patchedDependencies": {
      "mockjs@1.1.0": "patches/mockjs@1.1.0.patch"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "yarn lint",
      "yarn prettier:format"
    ],
    "*.{html,css,sass,scss,vue}": [
      "yarn lint:style"
    ]
  }
