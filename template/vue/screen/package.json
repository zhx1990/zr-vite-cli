    "naive-ui": "^2.34.4",
    "pinia": "^2.1.4",
    "postprocessing": "^6.32.2",
    "print-js": "^1.6.0",
    "qs": "^6.11.2",
    "socket.io-client": "^4.7.1",
    "supercluster": "^8.0.1",
    "swiper": "^9.4.1",
    "tweakpane": "^4.0.0",
